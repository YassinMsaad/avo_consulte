
				<div class="col-lg-8">
<input type="hidden" name="" value="{{ListAvocat|length}}" id="id">

{% for key in ListAvocat %}

				
					<div class="strip_list" dir="rtl" onclick="location.href='/محامي/{{key.Nomar|replace( {' ':'-'})}}/{{key.id}}';" style="cursor: pointer;">
						<figure>
                        <img src="../assets/images/{{key.img_url}}" alt="{{key.Nomar}}"></figure>

						<small>{{key.Nomfr}} </small>
                         <h3>{{key.Nomar}}</h3>

                         <ul>
							<li><a>الولاية </a></li>
							<li><p>{{key.Gouvernoratar}}</p></li>
							<li><a href="/محامي/{{key.Nomar|replace( {' ':'-'})}}/{{key.id}}/">إحجز موعد</a></li>
							
						</ul>

					</div>

					{% endfor %}
					
