		<script>

function showMoreQr() {


	var id =  document.getElementById("id").value ;
    var xmlhttp = new XMLHttpRequest();
    xmlhttp.onreadystatechange = function() {
      if (this.readyState == 4 && this.status == 200) {
        document.getElementById("renderQuestionReponse").innerHTML = this.responseText;
      }
    };
    xmlhttp.open("GET","renderQr/"+id,true);
    xmlhttp.send();
  
}

</script>
